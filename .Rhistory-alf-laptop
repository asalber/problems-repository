library(ggplot2)
set.seed(123)
n <- 20
x <- rbinom(n, 10, 0.5)
y <- rbinom(n, 10, 0.2)
z <- rbinom(n, 10, 0.8)
Score <- c(x, y, z)
Subject <- c(rep('X', n), rep('Y', n), rep('Z', n))
data <- data.frame(Score, Subject)
p <- ggplot(data, aes(x=Score, y=Subject)) + geom_boxplot(width=0.5)
p
